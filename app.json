{
    "expo": {
        "name": "The Moviesz",
        "slug": "themoviesz",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.themoviesz.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.themoviesz.app",
            "runtimeVersion": {
                "policy": "appVersion"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "scheme": "mama-safi",
        "experiments": {
            "tsconfigPaths": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "d06f88b7-1d9e-4bb7-993d-c313c581aadc"
            }
        },
        "updates": {
            "url": "https://u.expo.dev/d06f88b7-1d9e-4bb7-993d-c313c581aadc"
        }
    }
}
